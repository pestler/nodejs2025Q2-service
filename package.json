{
  "name": "home-library",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testMatch \"<rootDir>/*.spec.ts\" --noStackTrace --runInBand",
    "test:auth": "cross-env TEST_MODE=auth jest --testPathIgnorePatterns refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:refresh": "cross-env TEST_MODE=auth jest --testPathPattern refresh.e2e.spec.ts --noStackTrace --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "swagger:convert": "ts-node -r tsconfig-paths/register src/swagger/swagger-converter.ts",
    "docker:start": "docker compose up",
    "docker:stop": "docker compose down",
    "docker:restart": "docker compose down && docker compose up --build",
    "docker:test": "docker exec app npm run test",
    "docker:test-all": "docker exec app npm run test && docker exec app npm run test:cov",
    "prisma:prisma-db": "prisma generate && prisma migrate deploy && nest start --watch",
    "docker:scout": "docker scout cves pestler/nodejs2024q3-service:app-v2 && docker scout cves pestler/nodejs2024q3-service:postgres"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.8.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "core-js": "^3.42.0",
    "dotenv": "^16.5.0",
    "http-status-codes": "^2.3.0",
    "minimatch": "^10.0.1",
    "mkdirp": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "superagent": "^10.2.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.23",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "glob": "^11.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
