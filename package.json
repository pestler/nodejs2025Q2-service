    "prisma": "^5.22.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^6.0.1",
    "rxjs": "^7.2.0",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^11.1.2",
