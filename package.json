  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testMatch \"<rootDir>/*.spec.ts\" --noStackTrace --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "swagger:convert": "ts-node -r tsconfig-paths/register src/swagger/swagger-converter.ts",
    "docker:build": "docker compose up --build",
    "docker:start": "docker compose up --watch",
    "docker:stop": "docker compose down",
    "docker:restart": "docker compose up --force-recreate --build",
    "prisma:start": "prisma generate &&  prisma migrate deploy && nest start",
    "docker:scout": "docker scout cves nodejs2025q2-service-app"
  },
